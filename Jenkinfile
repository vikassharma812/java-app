pipeline {
    agent any

    stages {
        stage("GIT CHECKOUT") {
            steps{
                git branch: 'main', url: 'https://github.com/vikassharma812/java-app.git'
            }
        }
    stage("Unit Test")
        {
            steps{
                echo "main test"
            }
        }
    stage("Unit Test")
        {
            steps{
                sh "mvn clean install"
            }
        }
    }
}
